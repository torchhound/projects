doubleThis x = x + x
doubleThat x = x * 2
quadThis x = doubleThis (doubleThat x)
tripleUs x y = x * 3 + y * 3
quadSmall x = if x > 100 then x else quadThis x
sling = "fade"
another = "this"
single = 'A'
someNums = [1,2,3,4,5]
otherNums = [6,7,8,9]
conCat x y = x ++ y
consCat x y = x:y
ind x y = x !! y

